<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
	
	<groups>
		<run>
			<include name="Regression"></include>
		</run>
	</groups>

	<parameter name="Browser" value="chrome"></parameter>
	<parameter name="OS" value="windows"></parameter>
	
	<listeners>
		<listener class-name="codenbox.utilities.ExtentReporterNG"></listener>
		<listener class-name="codenbox.utilities.TestTransformer"></listener>
	</listeners>
	
	<test thread-count="5" name="Test">
		<classes>
			<class name="codenbox.testCases.HomeTest" />
			<class name="codenbox.testCases.LoginTest" />
			<!-- <class name="codenbox.testCases.RegistrationTest" /> -->
		</classes>
	</test> <!--
	Test -->
</suite> <!--
Suite -->

